{
  "name": "tetrastack",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write . && eslint --fix .",
    "prepare": "husky",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv ./cloudflare-env.d.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:seed": "tsx scripts/seed.ts",
    "db:dataload": "tsx scripts/dataload.ts",
    "batch:generate": "tsx scripts/ai-batch-gen/ai-batch-generate.ts",
    "batch:status": "tsx scripts/ai-batch-gen/ai-batch-check-status.ts",
    "batch:import": "tsx scripts/ai-batch-gen/ai-batch-import.ts",
    "test": "npm run test:unit && npm run test:integration && npm run test:components && npm run test:e2e",
    "test:unit": "vitest run tests/unit",
    "test:integration": "vitest run tests/integration",
    "test:components": "vitest run --config vitest.config.components.ts",
    "test:e2e": "playwright test",
    "test:e2e:coverage": "cross-env E2E_COVERAGE=1 playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:agents": "AGENTS_TEST=1 vitest run tests/agents",
    "coverage": "vitest run --coverage",
    "coverage:e2e": "npm run test:e2e:coverage",
    "typecheck": "tsc --noEmit",
    "generate-meal": "tsx scripts/generate-meal.ts"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.23",
    "@ai-sdk/react": "^2.0.28",
    "@auth/drizzle-adapter": "^1.10.0",
    "@heroicons/react": "^2.2.0",
    "@libsql/client": "^0.15.10",
    "@opennextjs/cloudflare": "^1.3.0",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@types/d3": "^7.4.3",
    "@types/js-yaml": "^4.0.9",
    "ai": "^5.0.68",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "d3": "^7.9.0",
    "dotenv": "^17.2.1",
    "drizzle-orm": "^0.44.3",
    "drizzle-zod": "^0.8.3",
    "js-yaml": "^4.1.0",
    "lucide-react": "^0.525.0",
    "next": "15.3.5",
    "next-auth": "5.0.0-beta.29",
    "openai": "^6.7.0",
    "postcss": "^8.5.6",
    "posthog-js": "^1.258.6",
    "posthog-node": "^5.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "tailwind-merge": "^3.3.1",
    "uuid": "^13.0.0",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@faker-js/faker": "^10.1.0",
    "@playwright/test": "1.52.0",
    "@tailwindcss/postcss": "^4.1.11",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.19.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.13",
    "@vitest/ui": "^3.2.4",
    "better-sqlite3": "^12.2.0",
    "cross-env": "^10.1.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-drizzle": "^0.2.3",
    "eslint-plugin-playwright": "^2.2.2",
    "fishery": "^2.3.1",
    "happy-dom": "^20.0.10",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "tailwindcss": "^4.1.11",
    "tsx": "^4.20.6",
    "tw-animate-css": "^1.3.5",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "wait-on": "^9.0.3",
    "wrangler": "^4.25.1"
  }
}
